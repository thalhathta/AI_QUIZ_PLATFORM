    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.18.3",
    "validator": "^13.15.15"
  },
  "devDependencies": {
    "@types/node": "^24.6.0",
    "eslint": "^9.36.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "mongodb-memory-server": "^10.2.1",
